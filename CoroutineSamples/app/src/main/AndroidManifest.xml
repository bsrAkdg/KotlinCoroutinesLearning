<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bsrakdg.coroutinesamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SupervisorJobActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".TimeoutActivity" />
        <activity android:name=".JobsActivity" />
        <activity android:name=".AsyncAndAwaitActivity" />
        <activity android:name=".SequentialActivity" />
        <activity android:name=".FreezeUIActivity" />
        <activity android:name=".RunBlockingActivity" />
        <activity android:name=".GlobalScopeActivity" />
        <activity android:name=".StructuredConcurrencyActivity" />
    </application>

</manifest>